@model IEnumerable<AutoRent.Models.CarVendor>
<!-- Vendor Start -->
<div class="container-fluid py-5">
    <div class="container py-5">
        <h1 class="display  text-center mb-1" style="color:darkorange">Car Vendors</h1>
        <div class="owl-carousel vendor-carousel">
            @foreach (var item in Model)
            {
            <div class="bg-light p-4">
                <a asp-action="CarByVendor" asp-controller="Home" asp-route-id="@item.VendorId">
                    <img src="@Url.Content(item.VendorLogo)" alt="">
                </a>
            </div>
            }
        </div>
